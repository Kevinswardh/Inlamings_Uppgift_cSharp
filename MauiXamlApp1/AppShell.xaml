<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:views="clr-namespace:MauiXamlApp1.Views"
       xmlns:auth="clr-namespace:MauiXamlApp1.Views.AuthenticationPages"
       x:Class="MauiXamlApp1.AppShell"
       FlyoutBehavior="Disabled">
    <!-- Inaktiverar hamburgermenyn -->

    <!-- MainPage -->
    <ShellContent Title="" ContentTemplate="{DataTemplate views:MainPage}" Route="MainPage" />

    <!-- RegisterPage -->
    <ShellContent Title="" ContentTemplate="{DataTemplate auth:RegisterPage}" Route="RegisterPage" />
</Shell>
